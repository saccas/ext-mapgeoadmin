<?xml version="1.0"?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>Map geo admin / iframe</sheetTitle>
                <type>array</type>
                <el>
                    <settings.mapgeoadmin.url>
                        <label>Map geo admin url (is converted to embed)</label>
                        <config>
                            <type>input</type>
                            <size>255</size>
                        </config>
                    </settings.mapgeoadmin.url>
                    <settings.iframe.width>
                        <label>Width (px or %)</label>
                        <config>
                            <default>100%</default>
                            <type>input</type>
                            <size>10</size>
                        </config>
                    </settings.iframe.width>
                    <settings.iframe.height>
                        <label>Height (px or %)</label>
                        <config>
                            <default>300px</default>
                            <type>input</type>
                            <size>10</size>
                        </config>
                    </settings.iframe.height>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
